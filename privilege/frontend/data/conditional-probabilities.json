  "uncertainty_parameters": {
    "_note": "Beta distribution parameters for Monte Carlo simulation. Alpha and beta values represent concentration parameters for uncertainty in each estimate.",
    "default_confidence": {
      "high_confidence": {
        "alpha": 50,
        "beta": 50,
        "description": "Well-documented statistics with large sample sizes (e.g., World Bank country classifications)"
      },
      "moderate_confidence": {
        "alpha": 20,
        "beta": 20,
        "description": "Published aggregates but limited cross-tabulation (e.g., conditional probabilities estimated from marginals)"
      },
      "low_confidence": {
        "alpha": 10,
        "beta": 10,
        "description": "Estimates based on proxy indicators or extrapolation (e.g., historical trends)"
      }
    },
